{
  "name": "are-we-formatting-yet",
  "type": "module",
  "version": "0.0.1",
  "scripts": {
    "dev": "astro dev",
    "build": "astro build",
    "preview": "astro preview",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "biome": "biome check --fix",
    "biome:check": "biome ci",
    "typecheck": "pnpm '/^typecheck:.*/'",
    "typecheck:typescript": "tsc -p ./tsconfig.json",
    "typecheck:astro": "astro check"
  },
  "dependencies": {
    "astro": "^5.0.0"
  },
  "devDependencies": {
    "@astrojs/check": "0.9.6",
    "@biomejs/biome": "2.4.2",
    "prettier": "3.8.1",
    "prettier-plugin-astro": "0.14.1",
    "prettier-plugin-toml": "2.0.6",
    "prettier-plugin-yaml": "1.1.2",
    "typescript": "5.9.3"
  },
  "engines": {
    "node": ">=18",
    "pnpm": ">=8"
  },
  "packageManager": "pnpm@10.29.1"
}
